# =============================================================================
# Application Settings
# =============================================================================
NEXT_PUBLIC_NAT_WORKFLOW=NeMo Agent Toolkit
NEXT_PUBLIC_NAT_GREETING_TITLE="Hi, I'm NeMo Agent Toolkit"
NEXT_PUBLIC_NAT_GREETING_SUBTITLE="How can I assist you today?"
NEXT_PUBLIC_NAT_INPUT_PLACEHOLDER="Unlock NeMo Agent Toolkit knowledge and expertise"
NEXT_PUBLIC_NAT_WELCOME_MESSAGE_ON=false
NEXT_PUBLIC_NAT_PROMPT_SUGGESTIONS_ON=false
NEXT_PUBLIC_NAT_WEB_SOCKET_DEFAULT_ON=false
NEXT_PUBLIC_NAT_CHAT_HISTORY_DEFAULT_ON=false
NEXT_PUBLIC_NAT_RIGHT_MENU_OPEN=false
NEXT_PUBLIC_NAT_ENABLE_INTERMEDIATE_STEPS=true
NEXT_PUBLIC_NAT_SHOW_DATA_STREAM_DEFAULT_ON=false
NEXT_PUBLIC_NAT_ADDITIONAL_VIZ_DEFAULT_ON=false

# =============================================================================
# Backend Configuration
# =============================================================================
# REQUIRED: Your API server backend address
# Always include the protocol (http:// or https://)
NAT_BACKEND_URL=http://127.0.0.1:8000

# =============================================================================
# Proxy Configuration
# =============================================================================
# Web application port - Always access the UI at this port
# Default: http://localhost:PORT (check terminal startup logs if a port conflict occurs to get the correct port)
PORT=3000

# Internal Next.js server URL - Used by the proxy to forward UI requests.
# This is NOT the port you access in your browser. Only change if port 3099
# conflicts with another service. Must match the port in package.json.
NEXT_INTERNAL_URL=http://localhost:3099

# Public HTTP and WebSocket paths the browser uses to reach the gateway.
HTTP_PUBLIC_PATH=/api
WS_PUBLIC_PATH=/ws

# =============================================================================
# Other Configuration
# =============================================================================
NODE_ENV=development
NAT_DEFAULT_MODEL=
NAT_MAX_FILE_SIZE_STRING=5mb
RAG_UUID=
